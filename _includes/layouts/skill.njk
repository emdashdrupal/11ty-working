{% include "layouts/partials/header.njk" %}
{{ collections.all | eleventyNavigation | eleventyNavigationToHtml | safe }}
{% block content %}
    <div class="mr-12 float-right mt-6">
        <span class="icon fa-{{ FontAwesomeIcon }}"></span></div>
    <article class="mt-6">
        {{ content | safe }}
    </article>
{% endblock %}
<a href="../">Back</a>
{% set previousPost = collections.skills | getPreviousCollectionItem %}
{% set nextPost = collections.skills | getNextCollectionItem %}

{% if previousPost %}
    <a href="{{ previousPost.url }}">Previous: {{ previousPost.data.title }}</a>
{% endif %}
|
{% if nextPost %}
    <a href="{{ nextPost.url }}">Next: {{ nextPost.data.title }}</a>
{% endif %}
{% include 'layouts/partials/footer.njk'%}


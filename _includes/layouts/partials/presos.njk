{% for presentation in filteredPresentations | sort(false, false, 'year') | reverse %}
  <ul class="presentation mb-4">
    <li>
    {% if presentation.link %}
      <a href="{{ presentation.link }}">
        <i class="text-lg">{{ presentation.title }}</i></a> {{ presentation.type }}

    {% else %}
      <i class="text-lg">{{ presentation.title }}</i> {{ presentation.type }}
    {% endif %}
    <span class="text-sm text-jet-600">({{ presentation.year }})</span>
    <p class="text-sm">{{ presentation.venue }} </p>
    </li>
  </ul>
{% endfor %}

{% if collections.all %}
{% set previousPost = collections.skills | getPreviousCollectionItem(item) %}
{% set nextPost = collections.skills | getNextCollectionItem(item) %}
{% if previousPost %}Previous: <a href="{{ previousPost.url }}">{{ previousPost.data.title }}</a>{% endif %}
{% if nextPost %}Next: <a href="{{ nextPost.url }}">{{ nextPost.data.title }}</a>{% endif %}
{% endif %}